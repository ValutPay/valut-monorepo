# Private key for deployment (required for deployment scripts)
PRIVATE_KEY=your_private_key_here

# RPC URL for the network you're deploying to
RPC_URL=your_rpc_url_here

# ValutOfframp address (required for initial deployment)
VALUT_OFFRAMP_ADDRESS=0x0000000000000000000000000000000000000000

# Proxy address (required for upgrade scripts)
PROXY_ADDRESS=0x0000000000000000000000000000000000000000

# New ValutOfframp address (required for UpdateValutOfframpAddress script)
NEW_VALUT_OFFRAMP_ADDRESS=0x0000000000000000000000000000000000000000

# Gas settings (optional)
GAS_PRICE=50000000000
GAS_LIMIT=6000000

# Etherscan API Key (for verification)
ETHERSCAN_API_KEY=your_etherscan_api_key_here
